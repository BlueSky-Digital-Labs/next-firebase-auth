{
  "compilerOptions": {
    "target": "es2019",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "stripInternal": true,
    "skipDefaultLibCheck": true,
    "baseUrl": "./src",
    "outDir": "./dist"
  }
}
